{
  "kind": "collectionType",
  "collectionName": "project_images",
  "info": {
    "singularName": "project-image",
    "pluralName": "project-images",
    "displayName": "ProjectImage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image_title": {
      "type": "string",
      "required": true
    },
    "image_src": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "project_slug": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project.project"
    },
    "cover_image": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
