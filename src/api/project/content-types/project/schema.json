{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "project_name": {
      "type": "string"
    },
    "project_year": {
      "type": "integer",
      "required": true,
      "default": 2018
    },
    "project_description": {
      "type": "blocks"
    },
    "postcards_sent": {
      "type": "integer",
      "default": 0
    },
    "project_type": {
      "type": "enumeration",
      "enum": [
        "Art Challenge",
        "Hobby Project",
        "Commissions",
        "Miscellaneous"
      ],
      "default": "Hobby Project",
      "required": true
    },
    "project_slug": {
      "type": "string"
    },
    "postcard_project": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "cover_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "project_images": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-image.project-image",
      "mappedBy": "project"
    }
  }
}
